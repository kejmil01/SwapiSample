<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:id="@+id/navGraph"
		app:startDestination="@id/homeFragment">
	<fragment
			android:id="@+id/homeFragment"
			android:name="net.fezzed.swapisample.ui.home.HomeFragment">
		<action
				android:id="@+id/action_homeFragment_to_testFragment"
				app:destination="@id/testFragment" />
	</fragment>

	<fragment
			android:id="@+id/testFragment"
			android:name="net.fezzed.swapisample.ui.test.TestFragment">
		<action
				android:id="@+id/action_testFragment_to_homeFragment"
				app:destination="@id/homeFragment" />

		<action
				android:id="@+id/action_testFragment_to_secondActivity"
				app:destination="@id/secondActivity" />
	</fragment>

	<activity
			android:id="@+id/secondActivity"
			android:name="net.fezzed.swapisample.ui.secondactivity.SecondActivity" />
</navigation>